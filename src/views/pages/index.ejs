    <dialog class="post-create" id="post-dia">
      <div class="post-create-titlebar">
        <div class="post-create-title">Posten Sie jetzt auf Klingt Gut</div>
        <div class="post-create-close" onclick="closeDialog()">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="16"
            height="16"
            fill="currentColor"
            class="bi bi-x-circle"
            viewBox="0 0 16 16"
          >
            <path
              d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16"
            />
            <path
              d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708"
            />
          </svg>
        </div>
      </div>
      <div class="post-create-content">
        <label for="post-creation-title"
          >Geben Sie ihren Post einen Titel</label
        >
        <input
          class="post-create-input"
          type="text"
          id="post-creation-title"
          placeholder="Ein aufregender Titel..."
        />
        <label for="post-creation-content"
          >Inhalt erweckt deinen Post zum lesen
        </label>
        <textarea
          class="post-create-textarea"
          minlength="20"
          maxlength="5000"
          id="post-creation-content"
        >
Hallo</textarea
        >
        <div class="upload-area">
          <div class="upload-area-input-container">
            <div class="upload-icon" name="type" id="upload-type">
              <div class="upload-icon-icon" id="upload-icon-icon">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="16"
                  height="16"
                  fill="currentColor"
                  class="bi bi-question"
                  viewBox="0 0 16 16"
                >
                  <path
                    d="M5.255 5.786a.237.237 0 0 0 .241.247h.825c.138 0 .248-.113.266-.25.09-.656.54-1.134 1.342-1.134.686 0 1.314.343 1.314 1.168 0 .635-.374.927-.965 1.371-.673.489-1.206 1.06-1.168 1.987l.003.217a.25.25 0 0 0 .25.246h.811a.25.25 0 0 0 .25-.25v-.105c0-.718.273-.927 1.01-1.486.609-.463 1.244-.977 1.244-2.056 0-1.511-1.276-2.241-2.673-2.241-1.267 0-2.655.59-2.75 2.286m1.557 5.763c0 .533.425.927 1.01.927.609 0 1.028-.394 1.028-.927 0-.552-.42-.94-1.029-.94-.584 0-1.009.388-1.009.94"
                  />
                </svg>
              </div>
              <div class="upload-options">
                <div
                  class="upload-option"
                  id="spotify"
                  onclick="changeUploadType('spotify')"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    fill="#1DB954"
                    class="bi bi-spotify"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M8 0a8 8 0 1 0 0 16A8 8 0 0 0 8 0m3.669 11.538a.5.5 0 0 1-.686.165c-1.879-1.147-4.243-1.407-7.028-.77a.499.499 0 0 1-.222-.973c3.048-.696 5.662-.397 7.77.892a.5.5 0 0 1 .166.686m.979-2.178a.624.624 0 0 1-.858.205c-2.15-1.321-5.428-1.704-7.972-.932a.625.625 0 0 1-.362-1.194c2.905-.881 6.517-.454 8.986 1.063a.624.624 0 0 1 .206.858m.084-2.268C10.154 5.56 5.9 5.419 3.438 6.166a.748.748 0 1 1-.434-1.432c2.825-.857 7.523-.692 10.492 1.07a.747.747 0 1 1-.764 1.288"
                    />
                  </svg>
                </div>
                <div
                  class="upload-option"
                  id="video"
                  onclick="changeUploadType('video')"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    fill="currentColor"
                    class="bi bi-camera-reels-fill"
                    viewBox="0 0 16 16"
                  >
                    <path d="M6 3a3 3 0 1 1-6 0 3 3 0 0 1 6 0" />
                    <path d="M9 6a3 3 0 1 1 0-6 3 3 0 0 1 0 6" />
                    <path
                      d="M9 6h.5a2 2 0 0 1 1.983 1.738l3.11-1.382A1 1 0 0 1 16 7.269v7.462a1 1 0 0 1-1.406.913l-3.111-1.382A2 2 0 0 1 9.5 16H2a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2z"
                    />
                  </svg>
                </div>
                <div
                  class="upload-option"
                  id="bild"
                  onclick="changeUploadType('picture')"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    fill="currentColor"
                    class="bi bi-image"
                    viewBox="0 0 16 16"
                  >
                    <path d="M6.002 5.5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0" />
                    <path
                      d="M2.002 1a2 2 0 0 0-2 2v10a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V3a2 2 0 0 0-2-2zm12 1a1 1 0 0 1 1 1v6.5l-3.777-1.947a.5.5 0 0 0-.577.093l-3.71 3.71-2.66-1.772a.5.5 0 0 0-.63.062L1.002 12V3a1 1 0 0 1 1-1z"
                    />
                  </svg>
                </div>
              </div>
            </div>
            <input
              class="upload-input"
              type="text"
              placeholder="Link zur Resource"
              id="upload-input"
            />
          </div>
        </div>
      </div>
      <button onclick="createPost()">Erstellen</button>
    </dialog>
    <button class="post-send" onclick="showDialog()" type="button">
      <div class="post-send-img">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="16"
          height="16"
          fill="currentColor"
          class="bi bi-send-fill"
          viewBox="0 0 16 16"
